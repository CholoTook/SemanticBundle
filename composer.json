{
	"name": "mediawiki/semantic-bundle",
	"type": "mediawiki-extension",
	"description": "Bundle extension that installs and loads Semantic MediaWiki and associated extensions",
	"keywords": [
		"MediaWiki",
		"Semantic MediaWiki",
		"Result format",
		"wiki"
	],
	"homepage": "https://github.com/SemanticMediaWiki/SemanticBundle",
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "Jeroen De Dauw",
			"email": "jeroendedauw@gmail.com",
			"homepage": "https://www.entropywins.wtf/mediawiki"
		}
	],
	"support": {
		"source": "https://github.com/SemanticMediaWiki/SemanticBundle"
	},
	"require": {
		"php": ">=7.1",
		"composer/installers": "^1.0.1",

		"mediawiki/semantic-media-wiki": "~3.0.0",

		"mediawiki/semantic-result-formats": "~3.0",
		"mediawiki/maps": "~7.4.0",
		"professional-wiki/modern-timeline": "~1.0",
		"phpoffice/phpspreadsheet": "~1.9",
		"phpoffice/phpexcel": "~1.8",

		"mediawiki/semantic-extra-special-properties": "~2.0",
		"mediawiki/semantic-compound-queries": "~2.1",

		"mediawiki/semantic-interlanguage-links": "~1.5",
		"mediawiki/semantic-cite": "~2.0",
		"mediawiki/semantic-glossary": "~3.0",
		"mediawiki/semantic-meta-tags": "~2.0",
		"mediawiki/semantic-breadcrumb-links": "~2.0",

		"mediawiki/page-forms": "~4.5"
	}
}
